obj_fn<-function(al,y){k<-length(y);
Z<-vector()
for (n in 1:k){
Z[n]=cos(2*pi*(al[1]*n+al[2]*n^2))+1i*sin(2*pi*(al[1]*n+al[2]*n^2))
}
t1<-t(Conj(Z))%*%y;
return(Re(t(Conj(y))%*%y-(abs(t1))^2/(k)))
}
